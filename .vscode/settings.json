{
	"editor.fontSize": 18,
	"editor.tabSize": 2,
	"editor.wordWrap": "off",
	"terminal.integrated.fontSize": 16,
	"editor.renderWhitespace": "all",
	"liveServer.settings.donotShowInfoMsg": true,
	"emmet.includeLanguages": {
		"javascript": "javascriptreact",
		"vue-html": "html",
		"razor": "html",
		"plaintext": "jade"
	},
	"editor.multiCursorModifier": "alt",
	"editor.largeFileOptimizations": false,
	"[javascript]": {
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"javascript.updateImportsOnFileMove.enabled": "always",
	"diffEditor.ignoreTrimWhitespace": false,
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"files.associations": {
		"*.css": "css",
		"*.js": "javascriptreact",
		"*.md": "markdown"
	},
	"workbench.iconTheme": "vs-seti",

	"editor.cursorStyle": "line-thin",
	"sync.gist": "d98b6a9f430f619f071b0d66b4a15a6e",
	"editor.wordWrapColumn": 120,

	"[css]": {
		"editor.defaultFormatter": "vscode.css-language-features"
	},
	"workbench.startupEditor": "newUntitledFile",
	"[scss]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"explorer.confirmDelete": false,
	"[markdown]": {},
	"eslint.alwaysShowStatus": true,
	"eslint.codeAction.disableRuleComment": {
		"enable": true
	},
	"eslint.format.enable": true,
	"eslint.lintTask.enable": true,
	"eslint.quiet": true,
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},

	"beautify.language": { "js": { "type": ["javascript", "json", "jsonc"], "filename": [".jshintrc", ".jsbeautifyrc"] }, "css": ["css", "scss"], "html": ["htm", "html", "blade"] },
	"prettier.useTabs": true,
	"beautify.config": "",
	"prettier.singleQuote": true,
	"eslint.run": "onSave",
	"eslint.workingDirectories": ["./src"],
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"prettier.jsxSingleQuote": true,
	"sync.autoDownload": true,
	"sync.autoUpload": true,
	"sync.forceDownload": true,
	"bracket-pair-colorizer-2.showBracketsInGutter": true,
	"bracket-pair-colorizer-2.showBracketsInRuler": true,
	"scss.lint.duplicateProperties": "warning",
	"javascript.preferences.quoteStyle": "single",
	"html.format.endWithNewline": true,
	"html-css-class-completion.enableEmmetSupport": true,
	"jake.autoDetect": "off",
	"liveSassCompile.settings.formats": [
		{
			"format": "compressed",
			"extensionName": ".min.css"
		}
	],
	"liveSassCompile.settings.includeItems": [],
	"liveServer.settings.AdvanceCustomBrowserCmdLine": "",
	"editor.formatOnSave": true,
	"prettier.printWidth": 200,

	"emmet.excludeLanguages": ["markdown", "Comment"],
	"emmet.syntaxProfiles": {},
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"html.format.indentInnerHtml": true,
	"html.format.wrapAttributes": "auto",
	"prettier.arrowParens": "avoid",
	"prettier.endOfLine": "auto",
	"phpformatter.composer": true,
	"phpfmt.detect_indent": true,
	"phpfmt.cakephp": true,
	"phpfmt.indent_with_space": 0,
	"settingsSync.ignoredSettings": ["-php.validate.executablePath"],
	"editor.accessibilityPageSize": 20,
	"security.workspace.trust.untrustedFiles": "open",
	"php.validate.executablePath": "C:/Program Files/php-8.0.9/php.exe",
	"phpfmt.php_bin": "\"C:\\Program Files\\php-8.0.9\\php.exe\"",
	"auto-rename-tag.activationOnLanguage": ["*"],
	"phpfmt.enable_auto_align": true,
	"phpfmt.smart_linebreak_after_curly": true,
	"phpfmt.passes": [],
	"beautify.ignore": "",
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"workbench.colorCustomizations": {
		"titleBar.activeForeground": "#ffffff",
		"titleBar.inactiveForeground": "#cecececc",
		"titleBar.activeBackground": "#740808",
		"titleBar.inactiveBackground": "#4d0404"
	},
	"workbench.colorTheme": "Default Dark+",
	"[php]": {
		"editor.defaultFormatter": "fterrag.vscode-php-cs-fixer"
	}
}
